{
  "restartable": "rs",
  "verbose": true,
  "exec": "build-pages.sh",
  "watch": [
    "."
  ],
  "ignoreRoot": [
    ".git/",
    ".nyc_output/",
    ".sass-cache/",
    "bower_components/",
    "coverage/",
    "node_modules/",
    "build/",
    "private/"
  ],
  "ignore": [],
  "env": {
    "NODE_ENV": "development"
  },
  "legacyWatch": true,
  "ext": "md,js,html,json,CNAME",
  "execMap": {
    "sh": "bash"
  }
}